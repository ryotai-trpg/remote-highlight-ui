{
  "id": "remote-highlight-ui",
  "title": "Remote Highlight UI",
  "description": "Allows users to highlight UI elements on others' screens.",
  "version": "2.3.1",
  "compatibility": {
    "minimum": "13",
    "verified": "13"
  },
  "esmodules": [
    "scripts/remote-highlight-ui.js",
    "scripts/hooks.js",
    "scripts/sockets.js"
  ],
  "socket": true,
  "styles": [
    "styles/remote-highlight-ui.css"
  ],
  "url": "https://github.com/shemetz/remote-highlight-ui",
  "manifest": "https://github.com/shemetz/remote-highlight-ui/raw/master/module.json",
  "download": "https://github.com/shemetz/remote-highlight-ui/archive/master.zip",
  "readme": "https://github.com/shemetz/remote-highlight-ui/blob/master/Readme.md",
  "bugs": "https://github.com/shemetz/remote-highlight-ui/issues",
  "license": "https://github.com/shemetz/remote-highlight-ui/blob/master/License.txt",
  "changelog": "https://github.com/shemetz/remote-highlight-ui/blob/master/Changelog.md",
  "relationships": {
    "requires": [
      {
        "id": "lib-wrapper",
        "type": "module",
        "manifest": "https://github.com/ruipin/fvtt-lib-wrapper/releases/latest/download/module.json",
        "compatibility": {
          "minimum": "1.12.7.0",
          "verified": "1.12.13.1"
        }
      }
    ]
  },
  "authors": [
    {
      "name": "shemetz",
      "discord": "shemetz",
      "email": "itamarcu+foundry@gmail.com",
      "reddit": "u/Shemetz"
    }
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    },
    {
      "lang": "ja",
      "name": "日本語",
      "path": "lang/ja.json"
    }
  ],
  "media": [
    {
      "type": "screenshot",
      "url": "https://raw.githubusercontent.com/shemetz/remote-highlight-ui/master/metadata/demo_1.gif",
      "caption": "Demo gif of Remote Highlight UI"
    },
    {
      "type": "icon",
      "url": "https://raw.githubusercontent.com/shemetz/remote-highlight-ui/master/metadata/icon.png",
      "caption": "Remote Highlight UI"
    }
  ]
}
